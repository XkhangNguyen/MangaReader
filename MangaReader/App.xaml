<Application x:Class="MangaReader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Utilities/DataTemplate.xaml"/>
                <ResourceDictionary Source="Styles/ChapterTitle.xaml"/>
                <ResourceDictionary Source="Styles/ChapterTitleDetail.xaml"/>
                <ResourceDictionary Source="Styles/CoverImage.xaml"/>
                <ResourceDictionary Source="Styles/CoverImageDetail.xaml"/>
                <ResourceDictionary Source="Styles/MangaTitle.xaml"/>
                <ResourceDictionary Source="Styles/MangaTitleDetail.xaml"/>
                <ResourceDictionary Source="Styles/Scene.xaml"/>
                <ResourceDictionary Source="Styles/MangaITem.xaml"/>
                <ResourceDictionary Source="Styles/LinkButton.xaml"/>
                <ResourceDictionary Source="Styles/TagButton.xaml"/>
                <ResourceDictionary Source="Styles/MangaDescriptionDisplay.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
